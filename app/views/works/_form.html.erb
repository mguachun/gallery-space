<%= form_for(@work) do |f| %>
    <%= f.label "Title:" %>
    <%=f.text_field :title %><br></br>
    <%= f.label(:artist) %>
    <%= f.text_field :artist %><br></br>
    <%= f.label(:year) %>
    <%= f.text_field :year%><br></br>
    <%= f.label(:medium) %>
    <%= f.text_field :medium %><br></br>

    <%= f.submit %>
<% end %>
